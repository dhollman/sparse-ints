molecule = $:ne4
molname = $molecule:name

ne10<Molecule>: (
  symmetry=auto
  units=angstrom
  { atoms    geometry } = {
     Ne  [ 0.0 0.0  0.0 ]
     Ne  [ 0.0 0.0  2.5 ]
     Ne  [ 0.0 0.0  5.0 ]
     Ne  [ 0.0 0.0  7.5 ]
     Ne  [ 0.0 0.0 10.0 ]
     Ne  [ 0.0 0.0 12.5 ]
     Ne  [ 0.0 0.0 15.0 ]
     Ne  [ 0.0 0.0 17.5 ]
     Ne  [ 0.0 0.0 20.0 ]
     Ne  [ 0.0 0.0 22.5 ]
  }
  name="Ne10"
)

ne3<Molecule>: (
  symmetry=auto
  units=angstrom
  { atoms    geometry } = {
     Ne  [ 0.0 0.0  0.0 ]
     Ne  [ 0.0 0.0  2.5 ]
     Ne  [ 0.0 0.0  5.0 ]
  }
  name="Ne3"
)

ne4<Molecule>: (
  symmetry=auto
  units=angstrom
  { atoms    geometry } = {
     Ne  [ 0.0 0.0  0.0 ]
     Ne  [ 0.0 0.0  5.0 ]
     Ne  [ 0.0 0.0 10.0 ]
     Ne  [ 0.0 0.0 15.0 ]
  }
  name="Ne4wide"
)

h2<Molecule>: (
  symmetry=auto
  unit = angstrom
  { atoms geometry} = {
    H  [  0.0  0.0  -0.35 ]
    H  [  0.0  0.0   0.35 ]
   }
   name="h2"
)


h2o<Molecule>: (
  symmetry=auto
  unit = angstrom
  { atoms geometry} = {
    H  [   1.5  0.0  -0.3 ]
    H  [  -1.5  0.0  -0.3 ]
    O  [   0.0  0.0   1.0 ]
   }
   name="h2o"
)

currentbas<GaussianBasisSet>: (
  molecule = $:molecule
  name = "cc-pVDZ-F12"
)

auxbas<GaussianBasisSet>: ( 
  molecule = $:molecule
  name = "cc-pVDZ-F12-CABS"
)

hf<CLHF>:(
  molecule=$:molecule
  basis=$:currentbas
  value_accuracy=1e-6
  integrals<IntegralLibint2>:()
)

all: (
   hf = $:hf
   basis = $:currentbas
   output_dir = "matrices"
   world<WavefunctionWorld>:(
	   wfn = $..:hf
   )
   aux_basis = $:auxbas
   corr_param = 1.0
   corr_type = "stg-6g"
   debug = no
   do_eri = yes
   do_f12 = no
   do_f12g12 = no
   do_P = yes
   do_Q = yes
   do_O = yes
   max_only = yes
   molname = $:molname
 )

