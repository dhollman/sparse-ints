h<Molecule>: (
  { atoms    geometry } = {
     H  [ 0.0 0.0 0.0 ]
  }
)

currentbas<GaussianBasisSet>: (
  molecule = $:molecule
  %name = "cc-pVDZ-F12"
  %name = "cc-pvtz-f12"
  %name = "cc-pvqz-f12"
  name = "cc-pVDZ"
  %name = "cc-pVTZ"
  %name = "cc-pVQZ"
)

auxbas<GaussianBasisSet>: ( 
  molecule = $:molecule
  name = "cc-pVDZ-F12-CABS"
  %name = "cc-pvtz-f12"
  %name = "cc-pvqz-f12"
  %name = "cc-pVDZ"
  %name = "cc-pVTZ"
  %name = "cc-pVQZ"
)

r12tech<R12Technology>: (
  corr_param = 1.0
  corr_type = "stg-6g"
)

hf<CLHF>:(
  molecule=$:molecule
  basis=$:currentbas
  value_accuracy=1e-6
  integrals<IntegralLibint2>:()
)

test: (
   hf = $:hf
   basis = $:currentbas
   storage = 10000000
   output_dir = "matrices"
   world<WavefunctionWorld>:(
	   wfn = $..:hf
   )
   aux_basis = $:auxbas
   r12tech = $:r12tech
   corr_param = 1.0
   corr_type = "stg-6g"
   use_symmetry = yes
   print_distribution = yes
   quartets_per_task = 25
   molname = $:molname
   cutoff = 14
 )

